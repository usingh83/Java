package day1.examples;

public class ExampleTwoDArray {

	public static void main(String[] args) {
		
		int[][] a=new int[4][3];
		for(int i=0;i<=4;i++)
		{
		  for(int j=0;j<=3;j++)
		    {
			a[i][j]=a[i][j]+10;
			}
		}
		
		/*for(int i=0;i<=4;i++)
		{
		  for(int j=0;j<=3;j++)
		    {
			System.out.println(a[i][j]);
			}
		}
*/

	}

}
